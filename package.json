{
  "name": "starposai-lpa",
  "version": "1.0.2",
  "description": "Local Print Agent para STARPOSAI (Electron + auto-update)",
  "private": true,

  "main": "src/main.js",

  "scripts": {
    "dev": "concurrently -k \"npm:serve\" \"npm:electron\"",
    "serve": "node src/scripts/dev-server.js",
    "electron": "wait-on http://127.0.0.1:9723/health && electron .",
    "start": "electron .",
    "dist": "electron-builder",
    "publish": "electron-builder --publish always",
    "list:printers": "node list-printers.js",
    "test:print": "node test-print.js \"Impresora Ticket 80mm (Ficticia)\" 1",
    "test-default-printer": "node test-default-printer.js"
  },

  "keywords": [],
  "author": "",
  "license": "ISC",

  "dependencies": {
    "cors": "^2.8.5",
    "electron-log": "^5.4.2",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "wait-on": "^8.0.4"
  }
}
